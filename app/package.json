{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "tsc -b && vite build --config vite.bundle-analyzer.config.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run src/**/__tests__/*.test.ts src/**/__tests__/*.test.tsx src/**/*.test.ts src/**/*.test.tsx --exclude src/__tests__/**",
    "test:integration": "vitest run src/__tests__/integration/**",
    "test:performance": "vitest run src/__tests__/performance/**",
    "test:accessibility": "vitest run src/__tests__/accessibility/**",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance && npm run test:accessibility",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "analyze": "npm run build:analyze && echo 'Bundle analysis complete. Check dist/bundle-analysis.json and dist/bundle-report.md'",
    "performance:test": "npm run build && npm run preview & sleep 3 && node -e \"console.log('Performance test complete. Check browser dev tools for metrics.')\" && killall node"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "@techstark/opencv-js": "^4.11.0-release.1",
    "@turf/turf": "^7.1.0",
    "@types/three": "^0.179.0",
    "decimal.js": "^10.4.3",
    "earcut": "^3.0.2",
    "localforage": "^1.10.0",
    "lucide-react": "^0.542.0",
    "martinez-polygon-clipping": "^0.7.2",
    "polygon-clipping": "^0.15.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tesseract.js": "^6.0.1",
    "three": "^0.179.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/earcut": "^3.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  }
}
